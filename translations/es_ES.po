msgstr "El intervalo máximo debe estar entre %s y %s segundos."

msgid "Minimum interval must be between %s and %s seconds."
msgstr "El intervalo mínimo debe estar entre %s y %s segundos."

msgid "AdvDefaultLifetime must be between %s and %s seconds."
msgstr "AdvDefaultLifetime debe estar entre %s y %s segundos."

msgid "AdvValidLifetime must be between %s and %s seconds."
msgstr "AdvValidLifetime debe estar entre %s y %s segundos."

msgid "AdvPreferredLifetime must be between %s and %s seconds."
msgstr "AdvPreferredLifetime debe estar entre %s y %s segundos."

msgid "AdvRDNSSLifetime must be between %s and %s seconds."
msgstr "AdvRDNSSLifetime debe estar entre %s y %s segundos."

msgid "AdvDNSSLLifetime must be between %s and %s seconds."
msgstr "AdvDNSSLLifetime debe estar entre %s y %s segundos."

msgid "AdvRouteLifetime must be between %s and %s seconds."
msgstr "AdvRouteLifetime debe estar entre %s y %s segundos."

