msgstr "ID"

msgid "IPv6-address"
msgstr "IPv6 アドレス"

msgid "Preferred Lifetime"
msgstr "優先する有効期間"

msgid "Valid Time"
msgstr "有効期間"

msgid "Prefix Delegation"
msgstr "プレフィックスデリゲーション"

msgid "id-assoc pd"
msgstr "id-assoc pd"

