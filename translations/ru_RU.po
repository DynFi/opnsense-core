msgstr "Настроенное действие правила"

msgid "Rulesets"
msgstr "Наборы правил"

msgid "SID"
msgstr "SID"

msgid "SNMP Agent Settings"
msgstr "Настройки агента SNMP"

msgid "SNMP password"
msgstr "Пароль SNMP"

msgid "SNMP port"
msgstr "Порт SNMP"

