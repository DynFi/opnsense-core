#!/usr/local/bin/php
<?php

require_once('config.inc');
require_once('interfaces.inc');
require_once('rrd.inc');
require_once('util.inc');
require_once('system.inc');

if (count($argv) > 1 && is_numeric($argv[1])) {
    // starting delayed.
    sleep($argv[1]);
}

system_webgui_configure(true);
enable_rrd_graphing(true);
