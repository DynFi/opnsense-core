<model>
    <mount>//OPNsense/RPZ/WhiteList</mount>
    <description>
        DNS filtering whitelist
    </description>
    <version>1.0.0</version>
    <items>
      <entries>
        <entry type=".\WhiteListField">
          <enabled type="BooleanField">
            <default>1</default>
            <Required>Y</Required>
          </enabled>
          <domain type="HostnameField">
            <Required>Y</Required>
            <multiple>N</multiple>
            <IpAllowed>N</IpAllowed>
            <HostWildcardAllowed>N</HostWildcardAllowed>
            <FqdnWildcardAllowed>Y</FqdnWildcardAllowed>
          </domain>
        </entry>
      </entries>
    </items>
</model>
