<model>
    <mount>//OPNsense/DFM</mount>    
    <description>
        DynFi Manager support module
    </description>
    <items>
      <settings>
        <managerIps type="TextField">
            <Required>N</Required>
        </managerIps>
        <interfaces type="InterfaceField">
            <Required>N</Required>
            <default></default>
            <multiple>Y</multiple>
            <filters>
                <enable>/^(?!0).*$/</enable>
            </filters>
        </interfaces>
        <authorizedKeys type="TextField">
            <Required>N</Required>
        </authorizedKeys>
        <tcpPort type="IntegerField">            
            <Required>N</Required>
        </tcpPort>
      </settings>
    </items>
</model>
